<p id="notice"><%= notice %></p>

<h1>Vehicles</h1>

<table>
  <thead>
    <tr>
      <th>Year model</th>
      <th>Brand</th>
      <th>Model</th>
      <th>Mv file no</th>
      <th>Plate no</th>
      <th>Engine no</th>
      <th>Chassis no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.year_model %></td>
        <td><%= vehicle.brand_id %></td>
        <td><%= vehicle.model_id %></td>
        <td><%= vehicle.mv_file_no %></td>
        <td><%= vehicle.plate_no %></td>
        <td><%= vehicle.engine_no %></td>
        <td><%= vehicle.chassis_no %></td>
        <td><%= link_to 'Show', vehicle %></td>
        <td><%= link_to 'Edit', edit_vehicle_path(vehicle) %></td>
        <td><%= link_to 'Destroy', vehicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle', new_vehicle_path %>
